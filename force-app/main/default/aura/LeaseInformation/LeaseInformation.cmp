<aura:component controller="TenantController" implements="flexipage:availableForRecordHome,force:hasRecordId" >
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
	<aura:attribute name="lease" type="Lease_Agreement__c"/>   
    <aura:attribute name="fields" type="String[]" default="['Name','Start Date', 'End Date']" />
  
 <div class="slds-card">
  <lightning:card title="Lease Agreement">
 
    
      <lightning:recordForm
        recordId="{!v.lease.Id}" objectApiName="Lease_Agreement__c"
        layoutType="Full"
       density="auto"
        mode="readonly" 
        columns="1"
                            fields="{!v.fields}" />
     
 </lightning:card>   
    </div>
</aura:component>