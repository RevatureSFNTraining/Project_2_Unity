<aura:component controller="TenantController" implements="flexipage:availableForRecordHome,force:hasRecordId" >
	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
	<aura:attribute name="prop" type="Property__c"/>
    <div class="slds-card ">
    <lightning:card title="Property">
    <lightning:recordForm
        recordId="{!v.prop.Id}" objectApiName="Property__c"
        layoutType="Full"
       density="auto"
        mode="readonly" 
        columns="1"/>
   </lightning:card>
    </div>
</aura:component>