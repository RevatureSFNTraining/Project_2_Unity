<aura:component  >
     <aura:attribute name="formatdate" type="Date"/>
    <aura:attribute name="tenant" type="tenant__c"/>
    <h1>Tenant Information </h1>
    <lightning:card title="{!v.tenant.name}" >
   
        <p class="slds-text-heading_medium slds-p-horizontal_small">
       	Name:  {!v.Tenant.Name}
        </p>
         <p class="slds-text-heading_medium slds-p-horizontal_small">
       	Phone:  {!v.Tenant.Primary_Phone__c}
        </p>
         <p class="slds-text-heading_medium slds-p-horizontal_small">
       	Email:  {!v.Tenant.Email__c}
        </p>
    </lightning:card>
</aura:component>